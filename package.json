{
  "name": "vite-vue3-template",
  "version": "0.0.1",
  "private": false,
  "description": "Vue3 + TypeScript template starter",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@antv/x6": "^2.18.1",
    "@vueuse/core": "^10.10.0",
    "axios": "^1.3.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.11",
    "element-plus": "^2.5.3",
    "events": "^3.3.0",
    "pinia": "^2.1.7",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "vue": "^3.3.4",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@typescript-eslint/typescript-estree": "^5.42.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "autoprefixer": "^10.4.19",
    "c8": "^7.11.3",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.7.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^9.4.0",
    "execa": "^4.0.2",
    "husky": "^7.0.0",
    "jsdom": "^19.0.0",
    "lint-staged": "^11.0.1",
    "minimist": "^1.2.6",
    "postcss": "^8.4.39",
    "prettier": "^2.6.2",
    "recast": "^0.20.4",
    "rimraf": "^3.0.2",
    "semver": "^7.3.7",
    "shx": "^0.3.4",
    "tailwindcss": "^3.4.4",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.17.4",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^3.1.3",
    "vitest": "^0.23.4",
    "vue": "^3.3.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*.scss": "prettier --write"
  }
}
